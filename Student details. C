include <stdio.h>
struct student {
    char name [50];
    int roll;
    float mark;
};
int main(){
    struct student s1;
    printf("Enter name;");
    scanf("%s", s1.name);
    printf("Enter roll number;");
    scanf("%d", &s1.roll);
    printf("Enter marks;");
    scanf("%f",&s1.mark);
    printf("\n student Details,\n");
    printf("Name;%s\nRoll;%d\nmarks;%, 2f\n", s1. name, s1. roll, s1. mark);
    
    return 0;
    
}
